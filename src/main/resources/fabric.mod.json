  "authors": [
    "Jake (aka notIrma)"
  ],
  "contributors": [
    "Notker"
  ],
  "contact": {
    "repo": "https://github.com/jb0s/blockgame-enhanced",
    "sources": "https://github.com/jb0s/blockgame-enhanced",
    "issues": "https://github.com/jb0s/blockgame-enhanced/issues"
  },
  "license": "unlicense",
  "icon": "assets/blockgame/textures/icon.png",
  "environment": "client",
  "accessWidener": "blockgameenhanced.accesswidener",
  "entrypoints": {
    "client": [
      "dev.jb0s.blockgameenhanced.BlockgameEnhancedClient"
    ],
    "main": [
      "dev.jb0s.blockgameenhanced.BlockgameEnhanced"
    ]
  },
  "mixins": [
    "BlockgameEnhanced.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
