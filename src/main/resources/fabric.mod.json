{
  "schemaVersion": 1,
  "id": "blockgameenhanced",
  "version": "${version}",
  "name": "Blockgame Enhanced",
  "description": "Fixes a lot of client-side issues and adds a ton of quality-of-life features for the Blockgame server.\nThe Blockgame Enhanced mod uses music from Heartbound, which was paid for and licensed from Pirate Software for non-commercial use.\nPlay Blockgame at: mc.blockgame.info",
  "authors": [
    "notIrma"
  ],
  "contributors": [
    "Notker",
    "klh_io"
  ],
  "contact": {
    "repo": "https://github.com/jb0s/blockgame-enhanced",
    "sources": "https://github.com/jb0s/blockgame-enhanced",
    "issues": "https://github.com/jb0s/blockgame-enhanced/issues"
  },
  "license": "unlicense",
  "icon": "assets/blockgame/textures/icon.png",
  "environment": "client",
  "accessWidener": "blockgameenhanced.accesswidener",
  "entrypoints": {
    "client": [
      "dev.jb0s.blockgameenhanced.BlockgameEnhancedClient"
    ],
    "main": [
      "dev.jb0s.blockgameenhanced.BlockgameEnhanced"
    ],
    "modmenu": [
      "dev.jb0s.blockgameenhanced.config.ModMenuCompat"
    ]
  },
  "mixins": [
    "BlockgameEnhanced.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}",
    "cloth-config": "*"
  },
  "breaks": {
  },
  "custom": {
    "modmenu": {
      "update_checker": false,
      "links": {
        "modmenu.discord": "https://discord.gg/piratesoftware"
      }
    }
  }
}
